{
  "name": "@turbovets/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:api": "nx serve api",
    "start:frontend": "nx serve dashboard",
    "start:dashboard": "nx serve dashboard",
    "start:static": "cd static-frontend && python3 -m http.server 4201",
    "start:enhanced": "cd static-frontend && python3 -m http.server 4202",
    "build:api": "nx build api",
    "build:dashboard": "nx build dashboard",
    "build": "nx build api && nx build dashboard",
    "test:api": "nx test api",
    "test:dashboard": "nx test dashboard",
    "test:e2e": "nx e2e dashboard-e2e",
    "test": "nx test api && nx test dashboard",
    "seed:db": "cd api && npx ts-node src/seed.ts",
    "seed": "cd api && npx ts-node src/seed.ts",
    "lint": "nx lint",
    "lint:fix": "nx lint --fix",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "setup": "./setup.sh",
    "setup:windows": "setup.bat",
    "dev": "concurrently \"npm run start:api\" \"npm run start:frontend\"",
    "dev:static": "concurrently \"npm run start:api\" \"npm run start:static\"",
    "dev:enhanced": "concurrently \"npm run start:api\" \"npm run start:enhanced\"",
    "clean": "nx reset && rm -rf node_modules package-lock.json",
    "install:clean": "npm run clean && npm install --legacy-peer-deps",
    "build:libs": "nx build data && nx build auth",
    "prebuild": "npm run build:libs"
  },
  "private": true,
